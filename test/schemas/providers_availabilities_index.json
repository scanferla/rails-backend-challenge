{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["provider_id", "from", "to", "free_slots"],
  "properties": {
    "provider_id": { "type": "integer" },
    "from": { "type": "string" },
    "to": { "type": "string" },
    "free_slots": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["starts_at", "ends_at"],
        "properties": {
          "starts_at": { "type": "string" },
          "ends_at": { "type": "string" }
        }
      }
    }
  }
}
